<%@posts.each do |p| %>
<div style="border: solid;">
<%= image_tag p.img %>
<%=p.content%>
</div>
<li><a><%= link_to 'Edit Post',{:action => 'edit', :id => p.id} %></a></li>
<li><a><%= link_to 'Delete Post',{:action => 'delete', :id => p.id} %></a></li>
<%end%>
<li><a><%= link_to 'Home', root_path %></a></li>